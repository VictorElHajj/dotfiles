    "disableLigatures.mode": "Line",
    "editor.renderWhitespace": "none",
    "rubyTestExplorer.minitestDirectory": "./spec/",
    "rubyTestExplorer.minitestFilePattern": [
        "*_test.rb",
        "test_*.rb",
        "*_spec.rb"
    ],
    "ruby.rubocop.suppressRubocopWarnings": true,
    "git.autofetch": true,
    "vim.camelCaseMotion.enable": true,
    "vim.normalModeKeyBindings": [],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "u"
            ],
            "after": [],
            "commands": [
                {
                    "command": "undo"
                }
            ]
        },
        {
            "before": [
                "<C-r>"
            ],
            "after": [],
            "commands": [
                {
                    "command": "redo"
                }
            ]
        }
    ],
    "vim.easymotion": true,
    "vim.leader": "ยง",
    "vim.easymotionMarkerMargin": 15,
    "vim.easymotionMarkerForegroundColorTwoChar": "red",
    "vim.easymotionMarkerForegroundColorOneChar": "red",
    "diffEditor.ignoreTrimWhitespace": false,
    "vim.highlightedyank.enable": true,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.maxColumn": 100,
    "editor.formatOnSave": true,
    "vim.highlightedyank.color": "purple"
}
